   "source": [
    "array = list(map(int, input().split()))   # 7 5 9 0 3 1 6 2 4 8\n",
    "\n",
    "for i in range(len(array)):\n",
    "    min_idx = i\n",
    "    for j in range(i+1, len(array)):\n",
    "        if array[min_idx] > array[j]:\n",
    "            min_idx = j\n",
    "    array[min_idx], array[i] = array[i], array[min_idx]\n",
    "\n",
    "print(array)"
   ]

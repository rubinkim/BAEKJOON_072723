   "source": [
    "##### Top-Down : Recursion & Memoization"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "cache = [-1] * 91\n",
    "cache[0], cache[1] = 0, 1\n",
    "cnt = 0\n",
    "\n",
    "def f(n):\n",
    "    global cnt\n",
    "    cnt += 1\n",
    "    \n",
    "    if cache[n] == -1:\n",
    "        cache[n] = cache[n-1] + cache[n-2]\n",
    "    \n",
    "    return cache[n]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "print(int(input()))\n",
    "print(f\"cnt : {cnt}\")"
   ]
  }
